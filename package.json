    "dev": "npm run _dev && nodemon --require @babel/register ./src",
    "prod": "npm run _prod && npm run start-build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "npx sequelize db:seed:all",
    "flow": "flow"
  },
