  "main": "./build/index.js",
  "scripts": {
    "_prod": "export NODE_ENV=production",
    "_dev": "export NODE_ENV=development USER=tanjim",
    "build": "npx babel src --out-dir build",
    "build-watch": "npx babel src --watch --out-dir build",
    "start-build": "npm run build && node ./build",
