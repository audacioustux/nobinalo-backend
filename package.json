{
  "name": "server",
  "version": "1.0.0",
  "description": "nobinalo.com backend",
  "private": true,
  "main": "./build/index.js",
  "scripts": {
    "watch:ts": "tsc -w -p tsconfig.json --preserveWatchOutput",
    "build:ts": "tsc -p tsconfig.json",
    "build": "npm run build:ts",
    "start": "node ./build",
    "start:watch": "nodemon",
    "upgrade": "ncu",
    "docker:up": "docker-compose up -d",
    "docker:rm": "docker-compose rm -sfv",
    "docker:reset": "npm run docker:rm && npm run docker:up",
    "db": "knex --knexfile ./src/config/db.ts",
    "db:mkmgr": "knex migrate:make --",
    "db:latest": "knex migrate:latest",
    "db:up": "knex migrate:up",
    "db:rollback": "knex migrate:rollback --",
    "db:mkseed": "knex seed:make --",
    "db:seed": "knex --knexfile src/config/db.ts seed:run --",
    "db:reset": "npm run db:rollback --all && npm run db:latest && npm run db:seed",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "author": {
    "name": "Tanjim Hosain",
    "email": "audacioustux@protonmail.com"
  },
